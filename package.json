{
  "name": "@matrixai/quic",
  "version": "0.0.7-alpha.0",
  "bin": {
    "server": "dist/bin/server.js"
  },
  "author": "Roger Qiu",
  "description": "QUIC",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatrixAI/js-quic.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "napi": {
    "name": "quic"
  },
  "scripts": {
    "prepare": "tsc -p ./tsconfig.build.json",
    "prebuild": "node ./scripts/prebuild.js",
    "build": "shx rm -rf ./dist && tsc -p ./tsconfig.build.json",
    "version": "node ./scripts/version.js",
    "prepublishOnly": "node ./scripts/prepublishOnly.js",
    "ts-node": "ts-node",
    "test": "jest",
    "lint": "eslint '{src,tests,scripts,benches}/**/*.{js,ts}'",
    "lintfix": "eslint '{src,tests,scripts,benches}/**/*.{js,ts}' --fix",
    "lint-shell": "find ./src ./tests ./scripts -type f -regextype posix-extended -regex '.*\\.(sh)' -exec shellcheck {} +",
    "docs": "shx rm -rf ./docs && typedoc --gitRevision master --tsconfig ./tsconfig.build.json --out ./docs src",
    "bench": "rimraf ./benches/results && ts-node ./benches"
  },
  "dependencies": {
    "@matrixai/async-cancellable": "^1.1.0",
    "@matrixai/contexts": "^1.0.0",
    "@matrixai/async-init": "^1.8.4",
    "@matrixai/async-locks": "^4.0.0",
    "@matrixai/errors": "^1.1.7",
    "@matrixai/logger": "^3.1.0",
    "@matrixai/resources": "^1.1.3",
    "@matrixai/timer": "^1.1.0",
    "ip-num": "^1.5.0"
  },
  "optionalDependencies": {
    "@matrixai/quic-darwin-arm64": "0.0.7-alpha.0",
    "@matrixai/quic-darwin-x64": "0.0.7-alpha.0",
    "@matrixai/quic-linux-x64": "0.0.7-alpha.0",
    "@matrixai/quic-win32-x64": "0.0.7-alpha.0"
  },
  "devDependencies": {
    "@fast-check/jest": "^1.1.0",
    "@napi-rs/cli": "^2.15.2",
    "@peculiar/asn1-pkcs8": "^2.3.0",
    "@peculiar/asn1-schema": "^2.3.0",
    "@peculiar/asn1-x509": "^2.3.0",
    "@peculiar/webcrypto": "^1.4.0",
    "@peculiar/x509": "^1.8.3",
    "@swc/core": "^1.3.62",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "benny": "^3.7.1",
    "common-tags": "^1.8.2",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "jest-extended": "^3.0.1",
    "jest-junit": "^14.0.0",
    "prettier": "^2.6.2",
    "semver": "^7.3.7",
    "shx": "^0.3.4",
    "sodium-native": "^3.4.1",
    "systeminformation": "^5.18.5",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  }
}
